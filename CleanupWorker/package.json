{
  "name": "tmc-file-transfer-cleanup",
  "version": "2.0.0",
  "description": "Cleanup worker for TMC File Transfer - handles expired files and database maintenance",
  "type": "module",
  "main": "src/index-improved.js",
  "scripts": {
    "deploy": "wrangler deploy --env production",
    "deploy:preview": "wrangler deploy --env preview", 
    "dev": "wrangler dev --env preview",
    "dev:local": "wrangler dev --local --env preview",
    "tail": "wrangler tail --env production",
    "tail:preview": "wrangler tail --env preview",
    "trigger": "node scripts/trigger-cleanup.js",
    "test": "node scripts/test-cleanup.js"
  },
  "dependencies": {
    "@cloudflare/workers-types": "^4.20240821.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "typescript": "^5.0.0",
    "wrangler": "^3.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}